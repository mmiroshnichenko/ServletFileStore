<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>org.mmyroshnychenko.controller.UserController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserUpdateServlet</servlet-name>
    <servlet-class>org.mmyroshnychenko.controller.UserUpdateController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UserDeleteServlet</servlet-name>
    <servlet-class>org.mmyroshnychenko.controller.UserDeleteController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FileUpdateServlet</servlet-name>
    <servlet-class>org.mmyroshnychenko.controller.FileUpdateController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FileDeleteServlet</servlet-name>
    <servlet-class>org.mmyroshnychenko.controller.FileDeleteController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FileServlet</servlet-name>
    <servlet-class>org.mmyroshnychenko.controller.FileController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EventServlet</servlet-name>
    <servlet-class>org.mmyroshnychenko.controller.EventController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserUpdateServlet</servlet-name>
    <url-pattern>/user-update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserDeleteServlet</servlet-name>
    <url-pattern>/user-delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileServlet</servlet-name>
    <url-pattern>/file</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUpdateServlet</servlet-name>
    <url-pattern>/file-update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileDeleteServlet</servlet-name>
    <url-pattern>/file-delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EventServlet</servlet-name>
    <url-pattern>/event</url-pattern>
  </servlet-mapping>
</web-app>